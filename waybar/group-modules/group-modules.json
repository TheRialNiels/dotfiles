{
  "group/temperatureGroup": {
    "orientation": "horizontal",
    "modules": [
      "temperature#Nvme",
      "temperature#Nvme2",
      "custom/dot-separator",
      "temperature#Cpu",
      "temperature#Cpu2",
      "custom/dot-separator",
      "temperature#Gpu",
      "temperature#Gpu2"
    ]
  },
  "group/storageGroup": {
    "orientation": "horizontal",
    "modules": [
      "disk#Root",
      "disk#Root2",
      "custom/dot-separator",
      "disk#Home",
      "disk#Home2"
    ]
  },
  "group/hardwareGroup": {
    "orientation": "horizontal",
    "modules": [
      "cpu#Icon",
      "cpu#Text",
      "custom/dot-separator",
      "memory#Icon",
      "memory#Text"
    ]
  },
  "group/clockGroup": {
    "orientation": "horizontal",
    "modules": ["clock#Icon", "clock#Text"]
  },
  "group/powerGroup": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 300,
      "children-class": "not-memory",
      "transition-left-to-right": false
    },
    "modules": ["custom/power", "custom/sleep", "custom/lock", "custom/restart"]
  }
}
